html { margin: 0;padding: 0;width: 100%; background:#F8F8F8;height:100%; overflow: hidden;}
body {font-family: Arial, Helvetica, sans-serif;font-size: 12px;width: 100%;height:100%;margin:0; padding:0; }
table {border-collapse: collapse;border-spacing: 0;}
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,blockquote,p{padding:0; margin:0;}
ol,ul {list-style:none;}
li{list-style-type:none;}
img{vertical-align:top;border:0;}
h1,h2,h3,h4,h5,h6{font-size:inherit; font-weight:normal;}
address,cite,code,em,i{font-weight:normal; font-style:normal;}
.clearfix{*zoom:1;}
.clearfix:after{display:block; overflow:hidden; clear:both; height:0; visibility:hidden; content:".";}

.fl{ float:left;}
.fr{float:right;}

body{padding-top: 5px;}
.where{margin:5px 15px;}
.where .item{float:left;height: 25px;padding-right: 10px;width: 220px}
.where .item span{float:left;}
.where .item .txt{height: 25px;line-height: 25px;width: 90px;text-align: right;}
.where .item span .input{width: 124px;}
.where .item span .input1{width: 120px;}
.pv{margin: 2px 0 0 0;padding: 1px 10px;background: #f7f6f6;border: solid 1px #bdbdbd;cursor: pointer;}

.left{width: 220px;float:left;}
.right{width: 639px;width: 630px!important;float: left;margin-left: 2px;}
.left .name,.right .name{height: 25px;background: #c0c0c0;line-height: 25px;padding-left: 10px;color: #000;font-weight: bold;}
