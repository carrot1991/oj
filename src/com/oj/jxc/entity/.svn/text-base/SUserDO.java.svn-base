package com.oj.jxc.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 数据对象
 * @since 2014-02-26
 */
public class SUserDO implements Serializable {

    private static final long serialVersionUID = 139338908098648872L;

    /**
     * column s_user.uname  用户名
     */
    private String uname;

    /**
     * column s_user.pwd  密码
     */
    private String pwd;

    /**
     * column s_user.isvalid  是否有效。默认1 有效，0为无效
     */
    private Integer isvalid;

    /**
     * column s_user.createdate  记录创建时间
     */
    private Date createdate;

    /**
     * column s_user.isdelete  是否已删。默认0 有效，1为无效
     */
    private Integer isdelete;

    /**
     * column s_user.updatedate  更新时间
     */
    private Date updatedate;

    private String qq;
    private String email;
    private String tel;
    
    private String rname;
    
    public String getRname() {
		return rname;
	}

	public void setRname(String rname) {
		this.rname = rname;
	}

	public String getQq() {
		return qq;
	}

	public void setQq(String qq) {
		this.qq = qq;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public SUserDO() {
        super();
    }

    public SUserDO(String uname, String pwd, Integer isvalid, Date createdate, Integer isdelete, Date updatedate) {
        this.uname = uname;
        this.pwd = pwd;
        this.isvalid = isvalid;
        this.createdate = createdate;
        this.isdelete = isdelete;
        this.updatedate = updatedate;
    }

    /**
     * getter for Column s_user.uname
     */
    public String getUname() {
        return uname;
    }

    /**
     * setter for Column s_user.uname
     * @param uname
     */
    public void setUname(String uname) {
        this.uname = uname;
    }

    /**
     * getter for Column s_user.pwd
     */
    public String getPwd() {
        return pwd;
    }

    /**
     * setter for Column s_user.pwd
     * @param pwd
     */
    public void setPwd(String pwd) {
        this.pwd = pwd;
    }

    /**
     * getter for Column s_user.isvalid
     */
    public Integer getIsvalid() {
        return isvalid;
    }

    /**
     * setter for Column s_user.isvalid
     * @param isvalid
     */
    public void setIsvalid(Integer isvalid) {
        this.isvalid = isvalid;
    }

    /**
     * getter for Column s_user.createdate
     */
    public Date getCreatedate() {
        return createdate;
    }

    /**
     * setter for Column s_user.createdate
     * @param createdate
     */
    public void setCreatedate(Date createdate) {
        this.createdate = createdate;
    }

    /**
     * getter for Column s_user.isdelete
     */
    public Integer getIsdelete() {
        return isdelete;
    }

    /**
     * setter for Column s_user.isdelete
     * @param isdelete
     */
    public void setIsdelete(Integer isdelete) {
        this.isdelete = isdelete;
    }

    /**
     * getter for Column s_user.updatedate
     */
    public Date getUpdatedate() {
        return updatedate;
    }

    /**
     * setter for Column s_user.updatedate
     * @param updatedate
     */
    public void setUpdatedate(Date updatedate) {
        this.updatedate = updatedate;
    }

}