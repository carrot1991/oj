package com.oj.jxc.entity;

import java.io.Serializable;

/**
 * 数据对象
 * @since 2014-02-26
 */
public class SShopOrdItemPartDO implements Serializable {

    private static final long serialVersionUID = 139338906723678818L;

    /**
     * column s_shop_ord_item_part.itemid  自增序列号
     */
    private Integer itemid;

    /**
     * column s_shop_ord_item_part.goodsid  对应的商品编号-可能不同厂家
     */
    private Integer goodsid;

    /**
     * column s_shop_ord_item_part.amount  数量
     */
    private Integer amount;

    /**
     * column s_shop_ord_item_part.price  价格
     */
    private Integer price;

    public SShopOrdItemPartDO() {
        super();
    }

    public SShopOrdItemPartDO(Integer itemid, Integer goodsid, Integer amount, Integer price) {
        this.itemid = itemid;
        this.goodsid = goodsid;
        this.amount = amount;
        this.price = price;
    }

    /**
     * getter for Column s_shop_ord_item_part.itemid
     */
    public Integer getItemid() {
        return itemid;
    }

    /**
     * setter for Column s_shop_ord_item_part.itemid
     * @param itemid
     */
    public void setItemid(Integer itemid) {
        this.itemid = itemid;
    }

    /**
     * getter for Column s_shop_ord_item_part.goodsid
     */
    public Integer getGoodsid() {
        return goodsid;
    }

    /**
     * setter for Column s_shop_ord_item_part.goodsid
     * @param goodsid
     */
    public void setGoodsid(Integer goodsid) {
        this.goodsid = goodsid;
    }

    /**
     * getter for Column s_shop_ord_item_part.amount
     */
    public Integer getAmount() {
        return amount;
    }

    /**
     * setter for Column s_shop_ord_item_part.amount
     * @param amount
     */
    public void setAmount(Integer amount) {
        this.amount = amount;
    }

    /**
     * getter for Column s_shop_ord_item_part.price
     */
    public Integer getPrice() {
        return price;
    }

    /**
     * setter for Column s_shop_ord_item_part.price
     * @param price
     */
    public void setPrice(Integer price) {
        this.price = price;
    }

}